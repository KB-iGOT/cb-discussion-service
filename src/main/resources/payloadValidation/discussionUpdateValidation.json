{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "discussionId": {
      "type": "string"
    },
    "communityId": {
      "type": "string"
    },
    "type": {
      "type": "string",
      "enum": ["question"]
    },
    "title": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
  "mediaCategory": {
    "type": "object",
    "properties": {
      "docs": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "links": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "media": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    }
  }
    },
  "required": [
    "discussionId",
    "communityId"
  ],
  "additionalProperties": false
}